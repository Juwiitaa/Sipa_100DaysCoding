package day7;
public class Main {
	public static void main(String[] args) {
	
		int nilaiPertama = 20;
		int nilaiKedua = 23;
		System.out.println("Nilai Pertama : "+nilaiPertama);
		System.out.println("Nilai Kedua : "+nilaiKedua);
		
		System.out.print("Nilai rata-ratanya adalah : ");
		System.out.print((nilaiPertama+nilaiKedua)/2);
		
	}
}
