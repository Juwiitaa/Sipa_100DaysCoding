package day34;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        
          for (int i = 1; i <= 5; i++) {
            if (i % 2 == 0) {
                System.out.println(i + " adalah bilangan genap");
            } else {
                System.out.println(i + " adalah bilangan ganjil");
            }
        }
    }
}

