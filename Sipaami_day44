package day44;
import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		
		System.out.print("Masukkan angka : ");
		int angka = input.nextInt();
		
		if (angka % 2 == 0){
			for (int a = 1; a < angka; a+=2){	
				System.out.print(a+",");
			}
		} else {
			for (int a = 2; a < angka; a+=2){
				System.out.print(a+",");
			}
		}
		
	}
}
