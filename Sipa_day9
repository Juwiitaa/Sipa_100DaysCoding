package day9;
public class main{
	public static void main(String[ ] args) {
        /*
         =
		+=
		-=
		/=
		*=
		%=
		
			*/
		int angka, nilai;
		nilai = 10;
		angka = 8;
			System.out.println(angka);
    	angka= 56;
    	     System.out.println(angka);
		
		nilai = 4;
		angka = 7;
		    System.out.println(nilai);
		    angka += nilai;
		    System.out.println(nilai);
		    	
		angka  = 7;
		nilai = 4;
		    System.out.println(angka);
		   angka  -= nilai ;
		    System.out.println(angka);
		    
		angka = 8;
		nilai  = 2;
		    System.out.println(nilai);
		    angka /= nilai;
		    System.out.println(nilai);
	
		angka = 4;
      	nilai = 4;
		    System.out.println(angka);
		    angka *= nilai;
		    System.out.println(angka);
		    
		  angka = 5;
      	nilai = 2;
		    System.out.println(nilai);
		    angka %= nilai;
		    System.out.println(nilai);  
		    
		    
		
	}
	
}
