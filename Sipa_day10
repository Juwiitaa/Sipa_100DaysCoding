package day10;
public class Sipa {
	public static void main(String[] args) {
				
				//gunakan tipe data  variabel seperti String, int, double, char, boolean untuk mendefinisikan variabel seperti berikut
				
	// kita akan membuat deklarasi//			
								
				String nama ="Sipa ami";
				//tulis variabel di type data String dan seterusnya tapi harus sesuai dengan variavbel dan type datanya
		String alamat ;
		alamat ="Kalimantan selatan";
		int umur;
		umur = 18;
		double tinggi;
		tinggi = 1.60;
		char jeniskelamin; 
		jeniskelamin = 'p';
		boolean status; 
		status = true;
		
		//setelah itu prongram akan mencetak variabel yang anda masukan dengan System.out.println
		System.out.println(nama);
		System.out.println(alamat);
		System.out.println(umur);
		System.out.println(tinggi);
		System.out.println(jeniskelamin);
		System.out.println(status);
		
		
	}
}


